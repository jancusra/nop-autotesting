@using Nop.Web.Framework.UI
@model KSystem.Nop.Plugin.Misc.AutoTesting.Models.TaskReports.ExecutedTaskModel

@{
    NopHtml.SetActiveMenuItemSystemName("KSystem.AutoTesting.TaskReports");
}

@{
    Layout = "_AdminLayout";
}

<form asp-controller="TaskReports" asp-action="Detail" method="post">
    <div class="content-header clearfix">
        <h1 class="float-left">
            @T("KSystem.Nop.Plugin.Misc.AutoTesting.TaskReportDetail") - @Model.TaskName
            <small><i class="fa fa-arrow-circle-left"></i>@Html.ActionLink(T("KSystem.Nop.Plugin.Misc.AutoTesting.BackToList").Text, "List")</small>
        </h1>
        <div class="float-right">
            <span id="taskreports-delete" class="btn bg-red">
                <i class="fas fa-trash"></i>
                @T("Admin.Common.Delete")
            </span>
        </div>
    </div>
    @await Html.PartialAsync("_ListOfMessages", Model.MessageSearchModel)
</form>

<nop-delete-confirmation asp-model-id="@Model.Id" asp-button-id="taskreports-delete" />
